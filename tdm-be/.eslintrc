{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "tsconfig.dev.json"
    },
    "env": {
        "node": true
    },
    "plugins": [
        "import"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:import/recommended",
        "plugin:import/typescript"
    ],
    "settings": {
        "import/resolver": {
            "typescript": {
                "project": "tsconfig.dev.json"
            }
        }
    }
}
